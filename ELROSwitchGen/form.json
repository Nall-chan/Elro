{
    "elements":
            [
                {
                    "type": "Label",
                    "caption": "Config adresse here:"
                },
                {
                    "name": "Code",
                    "type": "ValidationTextBox",
                    "caption": "Intertechno Code"
                },
                {
                    "name": "CodeOn",
                    "type": "ValidationTextBox",
                    "caption": "ON Code"
                },
                {
                    "name": "CodeOff",
                    "type": "ValidationTextBox",
                    "caption": "OFF Code"
                },
                {
                    "name": "Repeat",
                    "type": "Select",
                    "caption": "Repeat code",
                    "options": [
                        {
                            "caption": "1",
                            "value": 1
                        },
                        {
                            "caption": "2",
                            "value": 2
                        },
                        {
                            "caption": "3",
                            "value": 3
                        },
                        {
                            "caption": "4",
                            "value": 4
                        }
                    ]
                }
            ],
    "actions":
            [
                {
                    "type": "Button",
                    "caption": "ON",
                    "onClick": "ELRO_SendSwitchGen($id, true);"
                },
                {
                    "type": "Button",
                    "caption": "OFF",
                    "onClick": "ELRO_SendSwitchGen($id, false);"
                }]
}